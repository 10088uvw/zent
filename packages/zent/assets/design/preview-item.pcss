.zent-design-preview-item {
  position: relative;
}

.zent-design-vue-preview,
.zent-design-react-preview {
  position: relative;
  overflow: hidden;

  &:before {
    display: block;
    content: '';
    position: absolute;
    top: 0;
    left: 0;
    width: 100%;
    height: 100%;
    z-index: 1;
  }
}
