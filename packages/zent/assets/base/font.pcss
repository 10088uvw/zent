@import '../theme/_font.pcss';

.zent-font-weight-normal {
  font-weight: $font-weight-normal;
}

.zent-font-weight-medium {
  font-weight: $font-weight-medium;
}

.zent-font-weight-bold {
  font-weight: $font-weight-bold;
}

.zent-font-normal {
  @mixin font-normal;
}

.zent-font-small {
  @mixin font-small;
}

.zent-font-large {
  @mixin font-large;
}

.zent-font-h1 {
  @mixin font-h1;
}

.zent-font-h2 {
  @mixin font-h2;
}

.zent-font-h3 {
  @mixin font-h3;
}

.zent-font-h4 {
  @mixin font-h4;
}
